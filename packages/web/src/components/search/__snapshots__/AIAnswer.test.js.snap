.emotion-0 select {
  font-family: -apple-system,BlinkMacSystemFont,"Segoe UI","Roboto","Noto Sans","Ubuntu","Droid Sans","Helvetica Neue",sans-serif;
}

.emotion-0 *,
.emotion-0 *:before,
.emotion-0 *:after {
  box-sizing: border-box;
}

.emotion-2 {
  position: relative;
  margin: 0 auto;
  padding: 10px;
  background-color: #fafafa;
  width: 100%;
  box-shadow: rgb(0 0 0 / 20%) 0px 0px 6px;
  border-radius: 6px;
  margin-bottom: 20px;
  background-color: #fff;
}

.emotion-2 .--trigger-message-wrapper {
  cursor: pointer;
}

<div
  className="emotion-0 emotion-1"
  style={Object {}}
>
  <div
    className="--ai-chat-box-wrapper emotion-2 emotion-3"
    style={Object {}}
  >
    <div
      aria-hidden="true"
      className="--trigger-message-wrapper"
      onClick={[Function]}
    >
      <span>
        Click here to ask AI
         
        <span
          aria-label="bulb"
          role="img"
        >
          🤖
        </span>
      </span>
    </div>
  </div>
</div>
`;

