{
    "presets": [
        [
            "env", {
                "targets": {
                    "browsers": ["last 2 versions", "safari >= 7"]
                },
                "modules": false,
                "loose": true
            }
        ],
        "react"
    ],
    "plugins": [
		"emotion",
		"transform-object-rest-spread",
		"transform-class-properties"
    ]
}
